{
    "manifest_version": 2,
    "name": "Service now helper",
    "version": "1.0",
    "description": "Helpful scripts to run on Service now page",
    "icons": {
        "48": "icons/border-48.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.servicenow.com/*",
                "*://*.google.com/*"
            ],
            "js": [
                "borderify.js",
                "reload.js"
            ]
        }
    ]
}
